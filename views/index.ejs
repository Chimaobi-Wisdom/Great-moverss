    <script>
    // Function to submit a new review
    async function updateReview() {
    const name = document.getElementById("textInput1").value.trim();
    const review = document.getElementById("textInput2").value.trim();

  // Ensure both fields are filled
  if (!name || !review) {
    alert("Please fill in both your name and review.");
    return;
  }

  // Example image URL (you could also allow users to upload or choose one)
  const imageUrl = "images/profile.png";

  try {
    // Send review to the server
    const response = await fetch("https://great-moverss.onrender.com/reviews", {
      method: "POST",
      headers: {
        "Content-Type": "application/json",
      },
      body: JSON.stringify({ name, review, imageUrl }), // Send image URL to the server
    });

    if (response.ok) {
      alert("Thank you for your review!");

      // Clear the input fields
      document.getElementById("textInput1").value = "";
      document.getElementById("textInput2").value = "";

      // Reload the latest review into the display
      const latestReview = await response.json(); // Assuming the server responds with the saved review object
      document.getElementById("displayParagraph1").innerText = latestReview.name;
      document.getElementById("displayParagraph2").innerText = latestReview.review;

      // Update the image
      const imageElement = document.getElementById("imagecirc");
      imageElement.style.backgroundImage = `url('${latestReview.imageUrl}')`;

      // Scroll to the reviews section
      document.getElementById("sec-cff3").scrollIntoView({ behavior: "smooth" });
    } else {
      alert("Failed to submit your review. Please try again.");
    }
  } catch (error) {
    console.error("Error submitting review:", error);
    alert("An error occurred. Please try again later.");
  }
}

// Function to load the latest review from the database on page load
async function loadReviews() {
  try {
    const response = await fetch("https://great-moverss.onrender.com/reviews");
    const reviews = await response.json();

    if (reviews.length > 0) {
      const latestReview = reviews[reviews.length - 1]; // Get the most recent review
      document.getElementById("displayParagraph1").innerText = latestReview.name;
      document.getElementById("displayParagraph2").innerText = latestReview.review;

      // Update the image
      const imageElement = document.getElementById("imagecirc");
      imageElement.style.backgroundImage = `url('${latestReview.imageUrl}')`;
    }
  } catch (error) {
    console.error("Error loading reviews:", error);
    alert("Failed to load reviews. Please try again.");
  }
}

// Load the latest review on page load
window.onload = loadReviews;


  </script>
  
